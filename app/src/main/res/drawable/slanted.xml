<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:top="0dp"
        android:bottom="0dp"
        >
        <rotate
            android:fromDegrees="65"
            android:toDegrees="65"
            android:pivotX="20%"
            android:pivotY="30%" >
            <shape
                android:shape="line"
                >
                <stroke
                    android:width="4dp"
                    android:color="#000" />
            </shape>
        </rotate>
    </item>
</layer-list>